.tag-list {
    display: flex;
}

.tag {
    border-radius: 23px;
    background-color: #f3f5f9;
    padding: 15px 23px;
    margin-right: 15px;
    &:last-child {
        margin: 0;
    }
    span {
        font-size: 15px;
        color: #464d62;
        opacity: 0.7;
        font-family: Open Sans SemiBold;
    }
}